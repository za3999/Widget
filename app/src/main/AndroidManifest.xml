<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="widget.cf.com.widget">

    <application
        android:name="widget.cf.com.widgetlibrary.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SwipeLayoutActivity"></activity>
        <activity android:name=".ColorPickerActivity"></activity>
        <activity android:name=".MainTableActivity"></activity>
        <activity android:name=".DragBubbleActivity"></activity>
        <activity android:name=".DeviceInfoActivity"></activity>
        <activity android:name=".MainIndicatorActivity"></activity>
        <activity android:name=".FaceActivity"></activity>
    </application>

</manifest>
